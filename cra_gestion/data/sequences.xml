<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    <!-- Secuencia para Incidencias -->
    <record id="seq_cra_incidencia" model="ir.sequence">
        <field name="name">Secuencia Incidencias CRA</field>
        <field name="code">cra.incidencia</field>
        <field name="prefix">INC/%(year)s/</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="number_next">1</field>
        <field name="implementation">standard</field>
    </record>

    <!-- Secuencia para Partes de Trabajo -->
    <record id="seq_cra_parte_trabajo" model="ir.sequence">
        <field name="name">Secuencia Partes de Trabajo CRA</field>
        <field name="code">cra.parte.trabajo</field>
        <field name="prefix">PARTE/%(year)s/</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="number_next">1</field>
        <field name="implementation">standard</field>
    </record>

</odoo>
